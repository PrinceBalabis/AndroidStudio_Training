<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:id="@+id/widget49"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent"
             android:gravity="center"
             android:orientation="vertical">

    <Button
            android:id="@+id/taxi_ring"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Ring taxibolag"/>

    <Button
            android:id="@+id/taxi_kopiera_telefonnummer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kopiera telefonnummer till urklipp"/>

    <Button
            android:id="@+id/taxi_hemsida"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Öppna hemsida i webbläsare"/>

</TableLayout>